<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.datasophon.dao.mapper.FrameInfoMapper">

    <!--    getFrameServiceByFrameCode-->
    <select id="getFrameInfoByFrameCode" resultType="com.datasophon.dao.entity.FrameInfoEntity">
        select *
        from t_ddh_frame_info
        where frame_code = #{frameCode}
    </select>
</mapper>